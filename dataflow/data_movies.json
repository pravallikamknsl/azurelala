{
	"name": "data_movies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "movies_blob",
						"type": "DatasetReference"
					},
					"name": "moviesrc"
				},
				{
					"dataset": {
						"referenceName": "moviesdb_blob",
						"type": "DatasetReference"
					},
					"name": "moviesdbsrc"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "cleaningmovies"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> moviesrc",
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          RottenTomato as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> moviesdbsrc",
				"moviesrc derive(titlenew = toStirng(left(title,length(title)-6))) ~> cleaningmovies",
				"cleaningmovies join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}